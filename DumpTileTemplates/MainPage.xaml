<Page
    x:Class="DumpTileTemplates.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:DumpTileTemplates">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="メッセージ" />
            <TextBox x:Name="TileText" Width="400" />
            <Button Content="タイルを更新" Click="UpdateTile_Click" />
            <Button Content="テンプレートの一覧をファイルとして保存する" Click="Click_Save" Margin="0,20,0,0" />
        </StackPanel>
    </Grid>

</Page>
